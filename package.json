  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:start": "cd car-line-back && docker-compose up -d postgres",
    "db:stop": "cd car-line-back && docker-compose down",
    "db:init": "cd car-line-back && npm run db:init",
    "start": "concurrently -n \"DB,BACKEND,FRONTEND\" -c \"blue,green,yellow\" \"npm run db:start\" \"cd car-line-back && npm run start:dev\" \"cd car-line-front && npm run dev\"",
    "start:backend": "cd car-line-back && npm run start:dev",
    "start:frontend": "cd car-line-front && npm run dev"
  },
